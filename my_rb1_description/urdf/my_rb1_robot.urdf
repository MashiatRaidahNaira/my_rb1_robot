<?xml version="1.0" ?>

<robot name="robot">

    <gazebo reference="base_link">
        <material>Gazebo/Black</material>
    </gazebo>
    <gazebo reference="front_caster">
        <material>Gazebo/Black</material>
    </gazebo>
    <gazebo reference="back_caster">
        <material>Gazebo/Black</material>
    </gazebo>
    <gazebo reference="left_wheel">
        <material>Gazebo/Black</material>
    </gazebo>
    <gazebo reference="right_wheel">
        <material>Gazebo/Black</material>
    </gazebo>
    <gazebo reference="front_laser">
        <material>Gazebo/Black</material>
    </gazebo>

    <!-- Base - Link -->
    <link name="base_link">
        <inertial>
            <mass value="21" /> 
            <origin xyz="0 0 0.15" rpy="0 0 0" />
            <inertia ixx="0.485625" ixy="0" ixz="0" iyy="0.485625" iyz="0" izz="0.65625" />
        </inertial>

        <collision>
            <geometry>
                <cylinder length="0.3" radius="0.25"/>
            </geometry>
        </collision>

        <visual>
            <geometry>
                <cylinder length="0.3" radius="0.25"/>
            </geometry>
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>
        </visual>
    </link>
    
    <!-- Joint - base / left wheel -->
    <joint name="joint_base_left_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0.0 0.2 -0.15" />
        <child link="left_wheel" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>

    <!-- Link - left wheel -->
    <link name="left_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00203125" ixy="0" ixz="0" iyy="0.00203125" iyz="0" izz="0.0003125"/>
        </inertial>
        <!--Considering length of wheel = 0.15-->
        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.15" radius="0.025"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.15" radius="0.025"/>
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1" />
            </material>
        </visual>
    </link>

    <!-- Joint - base / right wheel -->
    <joint name="joint_base_right_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0.0 -0.2 -0.15" />
        <child link="right_wheel" />
        <parent link="base_link" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>

    <!-- Link - right wheel -->
    <link name="right_wheel">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00203125" ixy="0" ixz="0" iyy="0.00203125" iyz="0" izz="0.0003125"/>
        </inertial>
        <!--Considering length of wheel = 0.15-->
        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.15" radius="0.025"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0" />
            <geometry>
                <cylinder length="0.15" radius="0.025"/>
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1" />
            </material>
        </visual>
    </link>

    <!-- Base - Footprint -->
    <link name="base_footprint"/>
    <!-- Joint - base / base footprint -->
    <joint name="joint_base_footprint" type="fixed">
        <origin rpy="0 0 0" xyz="0 0 0.15" />
        <child link="base_link" />
        <parent link="base_footprint" />
    </joint>

    <!-- Joint - caster wheel front-->
    <joint name="joint_caster_wheel_front" type="fixed">
        <origin rpy="0 0 0" xyz="0.1 0.0 -0.15" />
        <child link="front_caster" />
        <parent link="base_link" />
        <joint_properties damping="0.0" friction="0.0" />
    </joint>
    <!-- Link - caster wheel front -->
    <link name="front_caster">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00025" ixy="0" ixz="0" iyy="0.00025" iyz="0" izz="0.00025"/>
        </inertial>

        <!-- Front Caster wheel support -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0.0375" />
            <geometry>
                <sphere radius="0.05" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.0375" />
            <geometry>
                <sphere radius="0.05" />
            </geometry>
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>
        </visual>

        <!-- Caster wheel -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <material name="Grey">
                <color rgba="0.6 0.6 0.6 1" />
            </material>
        </visual>
    </link>

    <!-- Joint - caster wheel back-->
    <joint name="joint_caster_wheel_back" type="fixed">
        <origin rpy="0 0 0" xyz="-0.1 0.0 -0.15" />
        <child link="back_caster" />
        <parent link="base_link" />
        <joint_properties damping="0.0" friction="0.0" />
    </joint>
    <!-- Link - caster wheel back -->
    <link name="back_caster">
        <inertial>
            <mass value="1" />
            <origin xyz="0 0 0" rpy="0 0 0" />
            <inertia ixx="0.00025" ixy="0" ixz="0" iyy="0.00025" iyz="0" izz="0.00025"/>
        </inertial>

        <!-- Back Caster wheel support -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0.0375" />
            <geometry>
                <sphere radius="0.05" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.0375" />
            <geometry>
                <sphere radius="0.05" />
            </geometry>
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>
        </visual>

        <!-- Caster wheel -->
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
        </collision>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <sphere radius="0.025" />
            </geometry>
            <material name="Grey">
                <color rgba="0.6 0.6 0.6 1" />
            </material>
        </visual>
    </link>

    <!--Joint - Front Laser -->
    <joint name="joint_front_laser" type="fixed">
      <origin xyz="0.25 0.0 0.075" rpy="3.14159 0 0" />
      <parent link="base_link"/>
      <child link="front_laser" />
    </joint>
    <link name="front_laser">
      <inertial>
        <mass value="1e-5" />
        <origin xyz="0 0 0" rpy="0 0 0" />
        <inertia ixx="1.67e-8" ixy="0.0" ixz="0.0"
                 iyy="1.67e-8" iyz="0.0"
                 izz="1.67e-8" />
      </inertial>
      <collision>
		    <origin xyz="0 0 0" rpy="0 0 0"/>
		    <geometry>
			    <box size="0.1 0.1 0.1"/>
		    </geometry>
	    </collision>
	    <visual>
		    <origin xyz="0 0 0" rpy="0 0 0"/>
		    <geometry>
			    <box size="0.1 0.1 0.1"/>
		    </geometry>
            <material name="DarkBlue">
                <color rgba="0.2 0.2 0.4 1" />
            </material>
	    </visual>
    </link> 
</robot>